{
  "extends": ["eslint-config-airbnb-base"],
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "2020"
  },
  "rules": {
    "arrow-body-style": 0,
    "consistent-return": 0,
    "max-len": 0,
    "object-curly-newline": 0,
    "prefer-destructuring": 0,
    "no-use-before-define": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0
  }
}
